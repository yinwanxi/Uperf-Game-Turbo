{
  "meta": {
    "name": "e2100",
    "author": "yc@coolapk   ❤️吟惋兮❤"
  },
  "modules": {
    "switcher": {
      "switchInode": "/sdcard/Android/yc/uperf/cur_powermode.txt",
      "perapp": "/sdcard/Android/yc/uperf/perapp_powermode.txt",
      "hintDuration": {
        "idle": 0.0,
        "touch": 4.0,
        "trigger": 0.03,
        "gesture": 0.1,
        "switch": 0.4,
        "junk": 0.06
      }
    },
    "atrace": {
      "enable": false
    },
    "log": {
      "level": "info"
    },
    "input": {
      "enable": true,
      "swipeThd": 0.03,
      "gestureThdX": 0.03,
      "gestureThdY": 0.03,
      "gestureDelayTime": 2.0,
      "holdEnterTime": 1.0
    },
    "sfanalysis": {
      "enable": true,
      "renderIdleSlackTime": 0.2
    },
    "cpu": {
      "enable": true,
      "powerModel": [
        {
          "efficiency": 115,
          "nr": 4,
          "typicalPower": 0.32,
          "typicalFreq": 2.2,
          "sweetFreq": 1.4,
          "plainFreq": 1.2,
          "freeFreq": 0.7
        },
        {
          "efficiency": 320,
          "nr": 3,
          "typicalPower": 1.66,
          "typicalFreq": 2.8,
          "sweetFreq": 1.7,
          "plainFreq": 1.2,
          "freeFreq": 0.7
        },
        {
          "efficiency": 380,
          "nr": 1,
          "typicalPower": 3.18,
          "typicalFreq": 2.9,
          "sweetFreq": 1.9,
          "plainFreq": 1.4,
          "freeFreq": 0.8
        }
      ]
    },
    "sysfs": {
      "enable": true,
      "knob": {
        "llccddrMin0": "/sys/class/devfreq/soc:qcom,cpu0-llcc-ddr-lat/min_freq",
        "llccddrMin4": "/sys/class/devfreq/soc:qcom,cpu4-llcc-ddr-lat/min_freq",
        "llccddrRC0": "/sys/class/devfreq/soc:qcom,cpu0-llcc-ddr-lat/mem_latency/ratio_ceil",
        "llccddrRC4": "/sys/class/devfreq/soc:qcom,cpu4-llcc-ddr-lat/mem_latency/ratio_ceil",
        "cpusetTa": "/dev/cpuset/top-app/cpus",
        "cpusetFg": "/dev/cpuset/foreground/cpus",
        "cpusetBg": "/dev/cpuset/background/cpus",
        "cpusetSysBg": "/dev/cpuset/system-background/cpus",
        "cpusetRe": "/dev/cpuset/restricted/cpus",
        "cpuMax":"/sys/devices/platform/exynos-ufcc/ufc/cpufreq_max_limit",
        "cpuMin":"/sys/devices/platform/exynos-ufcc/ufc/cpufreq_min_limit",
        "gpuMin":"/sys/kernel/gpu/gpu_min_clock",
        "powerLimit":"/sys/devices/platform/xperf/prof/power_limit",
        "sca7Min":"/sys/devices/system/cpu/cpufreq/policy7/scaling_min_freq",
        "sca7Max":"/sys/devices/system/cpu/cpufreq/policy7/scaling_max_freq",
        "sca4Max":"/sys/devices/system/cpu/cpufreq/policy4/scaling_max_freq",
        "sca4Min":"/sys/devices/system/cpu/cpufreq/policy4/scaling_min_freq"
      }
    },
    "sched": {
      "enable": true,
      "cpumask": {
        "all": [
          0,
          1,
          2,
          3,
          4,
          5,
          6,
          7
        ],
        "c0": [
          0,
          1,
          2,
          3
        ],
        "c1": [
          4,
          5,
          6
        ],
        "c2": [
          7
        ]
      },
      "affinity": {
        "auto": {
          "bg": "",
          "fg": "",
          "idle": "",
          "touch": "",
          "boost": ""
        },
        "norm": {
          "bg": "",
          "fg": "all",
          "idle": "all",
          "touch": "all",
          "boost": "all"
        },
        "bg": {
          "bg": "",
          "fg": "c0",
          "idle": "c0",
          "touch": "c0",
          "boost": "c0"
        },
