    "performance": {
      "*": {
        "cpu.latencyTime": 0,
        "cpu.slowLimitPower": 999,
        "cpu.fastLimitPower": 999,
        "cpu.fastLimitCapacity": 999,
        "cpu.margin": 0.3,
        "cpu.burst": 0.3
      },
      "idle": {
        "sched.scene": "idle"
      },
      "touch": {
        "cpu.margin": 0.55,
        "sched.scene": "touch"
      },
      "trigger": {
        "cpu.margin": 0.6,
        "sched.scene": "touch"
      },
      "gesture": {
        "cpu.margin": 0.7,
        "sched.scene": "touch"
      },
      "junk": {
        "cpu.burst": 0.9,
        "sched.scene": "touch"
      },
      "switch": {
        "cpu.burst": 0.5,
        "sched.scene": "boost"
      }
    },
    "fast": {
      "*": {
        "cpu.latencyTime": 0,
        "cpu.slowLimitPower": 999,
        "cpu.fastLimitPower": 999,
        "cpu.fastLimitCapacity": 999,
        "cpu.margin": 1.2
      },
      "idle": {
        "sched.scene": "idle"
      },
      "touch": {
        "sched.scene": "touch"
      },
      "trigger": {
        "sched.scene": "touch"
      },
      "gesture": {
        "sched.scene": "touch"
      },
      "junk": {
        "sched.scene": "touch"
      },
      "switch": {
        "sched.scene": "boost"
      }
    }
  }
}